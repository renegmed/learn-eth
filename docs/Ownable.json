  "source": "pragma solidity ^0.4.18;\n\ncontract Ownable {\n    // state variables\n    address owner;\n\n    // modifiers\n    modifier onlyOwner() {\n        require(msg.sender == owner);\n        _; // this is a placeholder for the code that modifier is applied to\n    }\n\n    function Ownable() public {\n        owner = msg.sender;\n    }\n}",
